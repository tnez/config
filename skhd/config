# launch commands
cmd - return : /Applications/Kitty.app/Contents/MacOS/kitty --single-instance

# chunkwm
shift + cmd - return : chunkc tiling::window --toggle fullscreen
ctrl - h : chunkc tiling::window --focus west
ctrl - j : chunkc tiling::window --focus south
ctrl - k : chunkc tiling::window --focus north
ctrl - l : chunkc tiling::window --focus east
ctrl - left : chunkc tiling::window --warp west
ctrl - down : chunkc tiling::window --warp south
ctrl - up : chunkc tiling::window --warp north
ctrl - right : chunkc tiling::window --warp east

