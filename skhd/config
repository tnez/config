# launch commands
cmd - return : cat $HOME/.cwd | ~/.config/kitty/scripts/new-window.sh
shift + cmd - b : /Applications/Google\ Chrome.app/Contents/MacOS/Google\ Chrome
shift + cmd - s : /Applications/Slack.app/Contents/MacOS/Slack
shift + cmd - m : /Applications/Spotify.app/Contents/MacOS/Spotify

# chunkwm
shift + cmd - return : chunkc tiling::window --toggle fullscreen
ctrl - h : chunkc tiling::window --focus west
ctrl - j : chunkc tiling::window --focus south
ctrl - k : chunkc tiling::window --focus north
ctrl - l : chunkc tiling::window --focus east
ctrl - left : chunkc tiling::window --warp west
ctrl - down : chunkc tiling::window --warp south
ctrl - up : chunkc tiling::window --warp north
ctrl - right : chunkc tiling::window --warp east
ctrl + alt - right : chunkc tiling::window -d next; \
                     chunkc tiling::desktop -f next
ctrl + alt - left : chunkc tiling::window -d prev; \
                    chunkc tiling::desktop -f prev
ctrl + cmd - right : chunkc tiling::window --use-temporary-ratio 0.1 \
                     --adjust-window-edge east
ctrl + cmd - left : chunkc tiling::window --use-temporary-ratio 0.1 \
                    --adjust-window-edge west
ctrl + cmd - down : chunkc tiling::window --use-temporary-ratio 0.1 \
                    --adjust-window-edge south
ctrl + cmd - up : chunkc tiling::window --use-temporary-ratio 0.1 \
                  --adjust-window-edge north
ctrl + cmd - 0 : chunkc tiling::desktop --equalize

